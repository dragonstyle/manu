{
  "name": "Default Container",
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
  "customizations": {
    "vscode": {
      "extensions": [
        "quarto.quarto"
      ]
    }
  },
  "features": {
    "ghcr.io/rocker-org/devcontainer-features/r-apt:0": {
